{% extends 'admin/base.html.twig' %}

{% block body %}
    <h1>Show categoryAttribute</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ categoryAttribute.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ categoryAttribute.name }}</td>
            </tr>
            <tr>
                <th>Sku</th>
                <td>{{ categoryAttribute.sku }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ categoryAttribute.price }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ categoryAttribute.quantity }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ categoryAttribute.description }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('product_categoryattribute_show') }}">Back to the list</a>
        </li>


    </ul>
{% endblock %}
